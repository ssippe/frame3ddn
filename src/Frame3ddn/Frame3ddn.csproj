<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Company>Steelx</Company>
    <Authors>Steelx</Authors>
    <Description>A library for static structural analysis of 2D and 3D frames and trusses. This is an incomplete C# version of Frame3DD (sourced from http://frame3dd.sourceforge.net/).</Description>
    <PackageProjectUrl>https://github.com/ssippe/frame3ddn</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ssippe/frame3ddn</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/ssippe/frame3ddn/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>public</RepositoryType>
    <PackageTags>frame3dd, structual analysis</PackageTags>
    <Copyright>copyright Â© Steelx 2018</Copyright>
    <PackageId>Frame3ddn</PackageId>
    <Version>1.0.6</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageReleaseNotes>Changed errors to throw exceptions. Fixed some warning output. Fixed false positive "error in y-axis trapezoidal loads" error.</PackageReleaseNotes>
    <AssemblyVersion>1.0.7.0</AssemblyVersion>
    <FileVersion>1.0.7.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
  </PropertyGroup>

</Project>
